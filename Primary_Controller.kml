<kml xmlns="http://www.opengis.net/kml/2.2">
	<Document>


		<NetworkLinkControl>
			<minRefreshPeriod>0.01</minRefreshPeriod>         <!-- float -->
			<maxSessionLength>-1</maxSessionLength>          <!-- float -->
			<linkName>Primary_Controller</linkName>          <!-- string -->
		</NetworkLinkControl>

		<visibility>1</visibility>

		<NetworkLink>
			<name>Main_Network_Link</name>
			<refreshVisibility>1</refreshVisibility>
			<flyToView>0</flyToView>
			<Link>
				<href>Secondary_Controller.kml</href>
				<refreshMode>onInterval</refreshMode>       <!-- refreshModeEnum: onChange, onInterval, or onExpire -->
				<refreshInterval>0.01</refreshInterval>        <!-- float -->
				<viewRefreshMode>never</viewRefreshMode>    <!-- viewRefreshModeEnum: never, onStop, onRequest, onRegion -->
			</Link>
		</NetworkLink>


	</Document>
</kml>
